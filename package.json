{
  "name": "typing-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clear_assets & vue-cli-service build --mode production && rsync -va dist/* . && rm -rf dist/",
    "lint": "vue-cli-service lint",
    "clear_assets": "sh bin/clear_assets.sh"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "pascal-case": "^3.1.2",
    "ramda": "^0.27.1",
    "vue": "^3.1.2",
    "vue-head": "^2.2.0",
    "vue-loader": "^16.2.0",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.0.0-beta.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.0-beta.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-v-html": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
